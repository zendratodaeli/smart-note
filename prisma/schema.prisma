generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model Note {
  id String @id @default(uuid())
  title String
  content String?
  userId String
  organizationId String?
  memberId String?
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}